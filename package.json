{
  "name": "blis-sdk",
  "version": "0.0.0-development",
  "description": "Node SDK for Blis",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "prettier": "prettier",
    "runprettier": "prettier --write --no-semi --single-quote src/**/*.ts",
    "prepublishOnly": "tsc",
    "pretest": "tsc",
    "tsc": "tsc",
    "test": "jest",
    "setpackageversion": "tsc -p ./scripts/tsconfig.json && node ./scripts/setpackageversion.js",
    "verifypackagelock": "tsc -p ./scripts/tsconfig.json && node ./scripts/verifypackagelock.js",
    "ui": "node ./lib/blisUiRunner.js"
  },
  "author": "Microsoft BLIS Team",
  "license": "MIT",
  "dependencies": {
    "blis-models": "^0.125.0",
    "blis-ui": "^0.139.0",
    "botbuilder": "4.0.0-m1.4",
    "express": "^4.15.3",
    "node-cache": "^4.1.1",
    "offline-directline": "^1.1.7",
    "redis": "^2.7.1",
    "request": "^2.79.0",
    "restify": "^4.3.0",
    "restify-cors-middleware": "^1.1.0",
    "swagger-client": "^2.1.32",
    "tslib": "^1.5.0",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "@types/execa": "^0.8.1",
    "@types/express": "^4.11.0",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.1.0",
    "@types/node": "^8.0.28",
    "@types/node-cache": "^4.1.0",
    "@types/redis": "^2.6.0",
    "@types/request": "^2.0.3",
    "@types/restify": "^4.3.5",
    "@types/restify-cors-middleware": "^1.0.1",
    "@types/xmldom": "^0.1.29",
    "execa": "^0.9.0",
    "fs-extra": "^5.0.0",
    "http-server": "^0.9.0",
    "jest": "^22.1.4",
    "nodemon": "^1.11.0",
    "prettier": "^1.10.2",
    "typescript": "^2.4.2"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "testMatch": [
      "**/lib/**/?(*.)(spec|test).js?(x)"
    ]
  },
  "prettier": {
    "tabWidth": 4,
    "printWidth": 140
  },
  "publishConfig": {
    "tag": "next"
  }
}
